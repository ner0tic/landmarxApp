{
    "require": {
        "php":                                                    ">=5.3.2",
        "symfony/symfony":                                        "2.1.*",
        "twig/extensions":                                        "dev-master",
        
        "doctrine/mongodb-odm-bundle":                            "3.0.*",        

        "symfony/assetic-bundle":                                 "2.1.0",
        "kriswallsmith/assetic":                                  "1.1.x-dev",
        
        "sensio/generator-bundle":                                "2.1.*",
        "symfony/translation":                                    "*",
        "symfony/framework-bundle":                               "2.1.*",
        "sensio/distribution-bundle":                             "v2.1.1",
        
        "jms/aop-bundle":                                         "1.0.0.0",
        "jms/security-extra-bundle":                              "1.2.0",
        "jms/di-extra-bundle":                                    "1.1.*",
        
        "symfony/security-bundle":                                "2.1.*",
        "symfony/swiftmailer-bundle":                             "2.1.*",
        "symfony/monolog-bundle":                                 "2.1.*",

        "sensio/framework-extra-bundle":                          "2.1.*",

        "kriswallsmith/buzz":                                     "v0.7",
        
        "willdurand/geocoder":                                    ">=1.0.0",
        "egeloen/google-map-bundle":                              "1.0",
        "oh/google-map-form-type-bundle":                         "dev-master",
        
        "ner0tic/landmarx":                                       "*",
        "ner0tic/landmarx-bundle":                                "*"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle":                         "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
        "post-update-cmd": [
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ]
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/",
            "Landmarx\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "description": "Landmark tree node mapping system",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "minimum-stability": "dev"
}