{% extends '::base.html.twig' %}

{% block content %}
  <form action="{{ path('landmark_new') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.primary_category) }}
    {{ form_row(form.parent) }}

    {{ form_rest(form) }}

    <input type="submit" />
  </form>
{% endblock %}